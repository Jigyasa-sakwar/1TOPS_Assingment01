`timescale 1ns / 1ps

module graytobinary2_tb;
reg [3:0]gray;
wire [3:0]bin;

graytobinary2 dut(gray,bin);
initial begin
$monitor("gray=%b,bin=%b,time %0t",gray,bin,$time);
gray=4'b0000;#10
gray=4'b0001;#10
gray=4'b0011;#10
gray=4'b0101;#10
gray=4'b0111;#10
gray=4'b1011;#10
gray=4'b1101;#10
gray=4'b1111;#10
#10 $stop;
end
endmodule